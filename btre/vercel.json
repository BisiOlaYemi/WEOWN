{
	"builds": [
		{
			"src": "brte/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.9"
			}
		},
		{
			"src": "build.sh",
			"use": "@vercel/static",
			"config":{
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "brte/wsgi.py"
		}
	]
}
	